{
  "name": "tic-tac-contract",
  "version": "1.0.0",
  "description": "Tic Tac Toe game using Koa.js, TypeScript, Vue.js and boardgame.io",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit",
    "start": "node dist/server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc",
    "docs:serve": "typedoc --watch"
  },
  "dependencies": {
    "@gala-chain/api": "2.0.4",
    "@gala-chain/connect": "2.0.4",
    "boardgame.io": "^0.50.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.4",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "jsdom": "^24.0.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.15"
  }
}
